<%= render '/events/sidebar' %>

          <div class="icone-page"><img src="/images/historique.png"></div>
          <h1>Mes Proth√®ses</h1></div>

<%= render '/events/navbar' %>

<p id="notice"><%= notice %></p>



<table>
  <thead>
    <tr>
      <th>Nom</th>
      <th>Serie</th>
      <th>Date d'installation</th>
      <th>Expiry-date</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @gears.each do |gear| %>
      <tr>
        <td><%= gear.name %></td>
        <td><%= gear.serie %></td>
        <td><%= gear.installation-date %></td>
        <td><%= gear.expiry-date %></td>
        <td><%= gear.description %></td>
        <td><%= link_to 'Show', gear %></td>
        <td><%= link_to 'Edit', edit_gear_path(gear) %></td>
        <td><%= link_to 'Destroy', gear, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Gear', new_gear_path %>

